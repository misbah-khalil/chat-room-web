{
    "name": "chat-room",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "engines": {
        "node": "14.19.1",
        "npm": "6.14.16"
    },
    "dependencies": {
        "axios": "^0.21.2",
        "babel-plugin-inline-react-svg": "^2.0.1",
        "classnames": "^2.3.1",
        "jest": "^26.6.3",
        "next": "^11.1.4",
        "path": "^0.12.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-loader-spinner": "^5.3.4",
        "react-toastify": "^8.1.0",
        "sass": "^1.36.0"
    },
    "scripts": {
        "build": "next build",
        "serve-local": "next dev",
        "serve-build": "next start",
        "local": "dotenv -e ./environments/server/.local.env -v NODE_OPTIONS=--inspect npm run serve-local",
        "dev": "dotenv -e ./environments/server/.dev.env -v NODE_OPTIONS=--inspect npm run serve-build",
        "stage": "dotenv -e ./environments/server/.stage.env npm run serve-build",
        "production": "dotenv -e ./environments/server/.prod.env npm run serve-build",
        "lint:quiet": "eslint . --quiet --ext .js,.jsx,.ts,.tsx",
        "prettier:check": "prettier --check .",
        "start": "npm run build && npm run production",
        "test": "jest --passWithNoTests --watch"
    },
    "devDependencies": {
        "@react-native-community/eslint-config": "^2.0.0",
        "@testing-library/jest-dom": "^5.1.1",
        "@testing-library/react": "^11.1.1",
        "@types/axios": "^0.14.0",
        "@types/node": "^15.12.4",
        "@types/react": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "dotenv-cli": "^6.0.0",
        "eslint": "^7.31.0",
        "eslint-config-next": "^11.0.1",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-webpack-plugin": "^2.2.1",
        "style-loader": "^2.0.0",
        "typescript": "^4.3.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.40.0"
